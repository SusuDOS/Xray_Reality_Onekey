{
  "log": {
    "access": "/var/log/xray/access.log",
    "error": "/var/log/xray/error.log",
    "loglevel": "warning"
  },
  "inbounds": [
    {
      "tag": "freedom_ws_inbound",
      "port": 443,
      "protocol": "vless",
      "settings": {
        "clients": [
          {
            "id": "xx",
            "alterId": 0
          }
        ],
        "decryption": "none",
        "fallbacks": [
          {
            "dest": 60000,
            "alpn": "",
            "xver": 1
          },
          {
            "dest": 60001,
            "alpn": "h2",
            "xver": 1
          },
          {
            "dest": 60002,
            "path": "/randomGetStr",
            "xver": 1
          }
        ]
      },
      "streamSettings": {
        "network": "tcp",
        "security": "tls",
        "tlsSettings": {
          "minVersion": "1.2",
          "certificates": [
            {
              "certificateFile": "/usr/local/etc/xray/self_signed_cert.pem",
              "keyFile": "/usr/local/etc/xray/self_signed_key.pem"
            },
            {
              "certificateFile": "/ssl/xray.crt",
              "keyFile": "/ssl/xray.key"
            }
          ]
        }
      },
      "sniffing": {
        "enabled": true,
        "destOverride": [
          "http",
          "tls"
        ]
      }
    },
    {
      "tag": "ws_inbound_vless_fallback",
      "port": 60002,
      "listen": "127.0.0.1",
      "protocol": "vless",
      "settings": {
          "clients": [
              {
                  "id": "xx"
              }
          ],
          "decryption": "none"
      },
      "streamSettings": {
          "network": "ws",
          "security": "none",
          "wsSettings": {
              "acceptProxyProtocol": true,
              "path": "xx"
          }
      }
  }
],
  "outbounds": [
    {
      "tag": "freedom_ws_outbound",
      "protocol": "freedom"
    }
  ]
}
